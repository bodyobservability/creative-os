{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "HVLIEN Serum Base Export Receipt v1",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "schema_version",
    "run_id",
    "timestamp",
    "job",
    "status",
    "target_path",
    "bytes",
    "reasons"
  ],
  "properties": {
    "schema_version": {
      "type": "integer",
      "const": 1
    },
    "run_id": {
      "type": "string"
    },
    "timestamp": {
      "type": "string"
    },
    "job": {
      "type": "string",
      "const": "serum_base_export"
    },
    "status": {
      "type": "string",
      "enum": [
        "pass",
        "fail",
        "warn"
      ]
    },
    "target_path": {
      "type": "string"
    },
    "bytes": {
      "type": [
        "integer",
        "null"
      ]
    },
    "reasons": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}